package com.test;

public class Sort1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//初始
		int array[] = {23,11,7,22,12};
		
		//输出没有排序的数组
		for(int i:array){
			System.out.print(i+"  ");
		}
		System.out.println();
		
		//11,12,7,22,23
		//            x,x,x,x,x 
		//排序
		//外层循环是 这个数组要排多少次
		for(int i=0;i<array.length-1;i++){//i表示需要排序的趟数0-4
			for(int k=0;k<array.length-i-1;k++){//k表示排序的下标位置k:4>3>2>1>0
				System.out.println(array[k]);
			}
		}
		//输出数组
		for(int i:array){
			System.out.print(i+"  ");
		}
	}

}
