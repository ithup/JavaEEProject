package cn.ithuplion.dao;

public class Test {
	public static void main(String[] args) {
		int length = 5;
		
		for(int i=1;i<=length;i++){
			//打印出内容
			if(i==1){
				//第一行打印
				//打印0
				for(int j=0;j<length-1;j++){
					System.out.print(0);
				}
			}else if(i==length){
//				最后一行
			}else{
//				中间
				
			}
			
			//换行
			System.out.println();
		}
	}
}
