package com.test;

public class Sort1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//初始
		int array[] = {23,11,7,22,12};
		
		//输出没有排序的数组
		for(int i:array){
			System.out.print(i+"  ");
		}
		System.out.println();
		
		//11,12,7,22,23
		//排序
		//外层循环是 这个数组要排多少次
		for(int i=0;i<array.length-1;i++){
			//
			for(){
				
			}
		}
	}

}
